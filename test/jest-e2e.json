{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/hitomi/(.*)": "<rootDir>/../libs/hitomi/src/$1",
    "@app/hitomi": "<rootDir>/../libs/hitomi/src",
    "@twitter/twi-videos.net/(.*)": "<rootDir>/../libs/twi-videos.net/src/$1",
    "@twitter/twi-videos.net": "<rootDir>/../libs/twi-videos.net/src",
    "@twitter/twivideo.net/(.*)": "<rootDir>/../libs/twivideo.net/src/$1",
    "@twitter/twivideo.net": "<rootDir>/../libs/twivideo.net/src",
    "@twitter/uraakalist.com/(.*)": "<rootDir>/../libs/uraakalist.com/src/$1",
    "@twitter/uraakalist.com": "<rootDir>/../libs/uraakalist.com/src"
  }
}