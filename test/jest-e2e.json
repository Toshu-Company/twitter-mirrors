{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/hitomi/(.*)": "<rootDir>/../libs/hitomi/src/$1",
    "@app/hitomi": "<rootDir>/../libs/hitomi/src",
    "@twitter/twi-videos.net/(.*)": "<rootDir>/../libs/twi-videos.net/src/$1",
    "@twitter/twi-videos.net": "<rootDir>/../libs/twi-videos.net/src",
    "@twitter/twivideo.net/(.*)": "<rootDir>/../libs/twivideo.net/src/$1",
    "@twitter/twivideo.net": "<rootDir>/../libs/twivideo.net/src",
    "@twitter/uraakalist.com/(.*)": "<rootDir>/../libs/uraakalist.com/src/$1",
    "@twitter/uraakalist.com": "<rootDir>/../libs/uraakalist.com/src",
    "@twitter/uraaka-joshi.com/(.*)": "<rootDir>/../libs/uraaka-joshi.com/src/$1",
    "@twitter/uraaka-joshi.com": "<rootDir>/../libs/uraaka-joshi.com/src",
    "@app/axios/(.*)": "<rootDir>/../libs/axios/src/$1",
    "@app/axios": "<rootDir>/../libs/axios/src",
    "@twitter/twidouga.net/(.*)": "<rootDir>/../libs/twidouga.net/src/$1",
    "@twitter/twidouga.net": "<rootDir>/../libs/twidouga.net/src",
    "@twitter/yatv.net/(.*)": "<rootDir>/../libs/yatv.net/src/$1",
    "@twitter/yatv.net": "<rootDir>/../libs/yatv.net/src"
  }
}