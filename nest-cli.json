{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "assets": [
      "**/*.proto"
    ],
    "watchAssets": true,
    "webpack": true
  },
  "projects": {
    "twi-videos.net": {
      "type": "library",
      "root": "libs/twi-videos.net",
      "entryFile": "index",
      "sourceRoot": "libs/twi-videos.net/src",
      "compilerOptions": {
        "tsConfigPath": "libs/twi-videos.net/tsconfig.lib.json"
      }
    },
    "twivideo.net": {
      "type": "library",
      "root": "libs/twivideo.net",
      "entryFile": "index",
      "sourceRoot": "libs/twivideo.net/src",
      "compilerOptions": {
        "tsConfigPath": "libs/twivideo.net/tsconfig.lib.json"
      }
    },
    "uraakalist.com": {
      "type": "library",
      "root": "libs/uraakalist.com",
      "entryFile": "index",
      "sourceRoot": "libs/uraakalist.com/src",
      "compilerOptions": {
        "tsConfigPath": "libs/uraakalist.com/tsconfig.lib.json"
      }
    }
  }
}